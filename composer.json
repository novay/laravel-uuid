{
    "name": "novay/laravel-uuid",
    "description": "Laravel package to generate and to validate a UUID according to the RFC 4122 standard. UUID Versions 1, 3, 4 and 5 are supported.",
    "authors": [
        {
            "name" : "Christoph Kempen",
            "email" : "christoph@downsized.nl"
        },
        {
            "name" : "Jeremy Kenedy",
            "email" : "jeremykenedy@gmail.com"
        },
        {
            "name": "Noviyanto Rahmadi",
            "email": "novay@enterwind.com"
        }
    ],
    "keywords": [
            "UUID RFC4122"
    ],
    "homepage" : "https://github.com/novay/laravel-uuid",
    "license": "MIT",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev" : {
      "fzaninotto/faker": "1.5.*", 
      "phpunit/phpunit": "4.7.*"
    },
    "autoload" : {
        "psr-0" : {
            "Novay\\Uuid" : "src/"
        }
    },
    "support" : {
        "source" : "https://github.com/novay/laravel-uuid",
        "issues" : "https://github.com/novay/laravel-uuid/issues"
    },
   "extra": {
        "laravel": {
            "aliases": {
                "Uuid": "Novay\\Uuid\\UuidFacade"
            }
        }
    },
    "minimum-stability": "stable"
}